<p style="color: green"><%= notice %></p>

<% content_for :title, "User credits" %>

<h1>User credits</h1>

<div id="user_credits">
  <% @user_credits.each do |user_credit| %>
    <%= render user_credit %>
    <p>
      <%= link_to "Show this user credit", user_credit %>
    </p>
  <% end %>
</div>

<%= link_to "New user credit", new_user_credit_path %>
