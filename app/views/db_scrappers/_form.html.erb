<%= form_with(model: db_scrapper, url: project_db_scrappers_path) do |form| %>
  <% if db_scrapper.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(db_scrapper.errors.count, "error") %> prohibited this db_scrapper from being saved:</h2>

      <ul>
        <% db_scrapper.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :url, style: "display: block" %>
    <%= form.text_field :url %>
