    <%= form.label :meta_data, style: "display: block" %>
    <%= form.text_field :meta_data %>
  </div>

  <div>
    <%= form.label :source_provider, style: "display: block" %>
    <%= form.text_field :source_provider %>
  </div>

  <div>
    <%= form.label :sub_type, style: "display: block" %>
    <%= form.text_field :sub_type %>
  </div>

  <div>
    <%= form.label :response, style: "display: block" %>
    <%= form.text_field :response %>
  </div>

  <div>
    <%= form.label :fildered_response, style: "display: block" %>
    <%= form.text_field :fildered_response %>
  </div>

  <div>
    <%= form.label :parser_code, style: "display: block" %>
    <%= form.textarea :parser_code %>
  </div>

  <div>
    <%= form.label :final_response, style: "display: block" %>
    <%= form.text_field :final_response %>
  </div>

  <div>
    <%= form.label :knowledge_storage_cypher_code, style: "display: block" %>
